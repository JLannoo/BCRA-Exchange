{
  "name": "bcra-exchange",
  "version": "0.0.7",
  "description": "A crawler for the BCRA (Banco Central de la República Argentina) frontend to get official exchange rates by date",
  "type": "module",
  "types": "build/main.d.ts",
  "files": [
    "/build"
  ],
  "exports": {
    ".": {
      "import": "./build/main.js",
      "require": "./build/main.cjs",
      "default": "./build/main.js",
      "types": "./build/main.d.ts"
    }
  },
  "main": "build/main.js",
  "module": "build/main.js",
  "scripts": {
    "test": "vitest --watch",
    "test:ui": "vitest --ui",
    "build": "tsup src/main.ts --dts --format cjs,esm --clean --minify --out-dir build",
    "prepare": "npm run build"
  },
  "keywords": [
    "bcra",
    "exchange",
    "rate",
    "cambio",
    "banco",
    "central",
    "argentina",
    "peso",
    "dolar",
    "dólar",
    "euro"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "jsdom": "^22.0.0"
  },
  "devDependencies": {
    "@swc/core": "^1.3.61",
    "@types/jsdom": "^21.1.1",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "@vitest/ui": "^0.31.1",
    "eslint": "^8.41.0",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4",
    "vitest": "^0.31.1"
  }
}
